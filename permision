void main() {
  //permision

  int age = 18;
  bool havePermision = true;
  bool isAdult = age >= 18;
  String day = "Friday";
  
  //condition

  String result = (isAdult && havePermision) || (isAdult && day == "Friday")
      ? "Allow the person"
      : "This persnon has n permision to enter the club";
  print(result);
}
